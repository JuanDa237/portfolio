{
	"name": "backend",
	"version": "0.0.0",
	"scripts": {
		"start": "node dist/index.js",
		"start:dev": "nodemon build/",
		"build:dev": "tsc -w",
		"build:prod": "rm -rf ./build && rm -rf ./dist && tsc && mv ./build ./dist && cd ../frontend && npm run build:prod",
		"deploy": "git add ./dist && git add ./public && git commit -m \"Deploy\""
	},
	"dependencies": {
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"mongoose": "^5.12.2",
		"morgan": "^1.10.0",
		"nodemailer": "^6.5.0",
		"nodemon": "^2.0.7"
	},
	"devDependencies": {
		"@types/cors": "^2.8.10",
		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.11",
		"@types/mongoose": "^5.10.4",
		"@types/morgan": "^1.9.2",
		"@types/nodemailer": "^6.4.1"
	}
}